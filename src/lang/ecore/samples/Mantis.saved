    <eStructuralFeatures xsi:type="ecore:EReference" name="i_mantisRoot" ordered="false" lowerBound="1" eType="//MantisRoot" eOpposite="//MantisRoot/issues"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="project" ordered="false" lowerBound="1" eType="//ValueWithId" containment="true"/>
