    <eStructuralFeatures xsi:type="ecore:EAttribute" name="additionalInfo" ordered="false" unique="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dateSubmitted" ordered="false" unique="false" lowerBound="1"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="assignedTo" ordered="false" eType="//Person" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="projection" ordered="false" eType="//ValueWithId" containment="true"/>
