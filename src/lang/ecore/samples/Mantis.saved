    <eLiterals name="rt_child of" value="3"/>
    <eLiterals name="rt_duplicate of" value="4"/>
    <eLiterals name="rt_has duplicate" value="5"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MantisRoot">
