  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IsPropertyOfFactType"/>
  <eClassifiers xsi:type="ecore:EClass" name="AssociativeFactType"/>
  <eClassifiers xsi:type="ecore:EClass" name="CategorizationFactType"/>
  <eClassifiers xsi:type="ecore:EClass" name="ObjectificationFactType"/>
