    <eStructuralFeatures xsi:type="ecore:EReference" name="occursIn" ordered="false" lowerBound="1" eType="//AtomicFormulation"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="isOf" ordered="false" lowerBound="1" eType="//FactType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="bindsTo" ordered="false" lowerBound="1" eType="//BindableTarget"/>
