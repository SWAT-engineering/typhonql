  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NonNegativeInteger">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" ordered="false" unique="false" lowerBound="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FactType"/>
