    <eStructuralFeatures xsi:type="ecore:EReference" name="roleBinding2" ordered="false" lowerBound="1" eType="//RoleBinding"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NecessityFormulation"/>
