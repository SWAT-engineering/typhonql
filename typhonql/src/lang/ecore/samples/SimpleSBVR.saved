<?xml version="1.0" encoding="ASCII"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="SimpleSBVR" nsPrefix="simplesbvr">
  <eClassifiers xsi:type="ecore:EClass" name="Root">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" ordered="false" upperBound="-1" eType="//Element" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Element">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="objectName" ordered="false" unique="false" lowerBound="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Representation">
    <eStructuralFeatures xsi:type="ecore:EReference" name="meaning" ordered="false" lowerBound="1" eType="//Meaning" eOpposite="//Meaning/representations"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Designation">
    <eStructuralFeatures xsi:type="ecore:EReference" name="text" ordered="false" lowerBound="1" eType="//Text"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Text">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" ordered="false" unique="false" lowerBound="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Meaning">
    <eStructuralFeatures xsi:type="ecore:EReference" name="representations" ordered="false" upperBound="-1" eType="//Representation" eOpposite="//Representation/meaning"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Concept"/>
  <eClassifiers xsi:type="ecore:EClass" name="NounConcept"/>
  <eClassifiers xsi:type="ecore:EClass" name="ObjectType">
    <eStructuralFeatures xsi:type="ecore:EReference" name="specializes" ordered="false" eType="//ObjectType" eOpposite="//ObjectType/generalizes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="generalizes" ordered="false" upperBound="-1" eType="//ObjectType" eOpposite="//ObjectType/specializes"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="IndividualConcept">
    <eStructuralFeatures xsi:type="ecore:EReference" name="instanceOf" ordered="false" eType="//ObjectType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DataType"/>
